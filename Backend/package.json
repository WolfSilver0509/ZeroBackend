{
  "name": "sms-sender-ts",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "read -p 'pm2 server name: ' NAME && pm2 start dist/server/www.js --name=$NAME --watch",
    "dev": "./.bin/starter.sh",
    "build": "pnpm i && tsc",
    "pr-migrate": "read -p 'name of migrate: ' NAME && npx prisma migrate dev --name $NAME",
    "pr-generate": "prisma generate",
    "db-reset": "prisma db push --force-reset",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "trace-sync-io": "node --trace-sync-io ./dist/server/www.js",
    "refresh": "rm -rf ./node_modules ./pnpm-lock.yaml && ./.pnpm_call/node_modules/.bin/pnpm i && echo '\\x1b[32mSuccessful refresh\\x1b[0m' || echo '\\x1b[31mRefresh failed\\x1b[0m'",
    "test": "jest --config jestconfig.json",
    "chrome-inspect": "node --inspect dist/server/www.js"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/express-enforces-ssl": "^1.1.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.31",
    "@types/serve-favicon": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "jest": "^28.1.0",
    "newman": "^5.3.2",
    "nock": "^13.2.4",
    "npm-check": "^5.9.2",
    "prisma": "^3.13.0",
    "prisma-dbml-generator": "^0.9.1",
    "prisma-docs-generator": "^0.5.0",
    "ts": "^0.2.2",
    "ts-node": "^10.8.0"
  },
  "dependencies": {
    "@prisma/client": "^3.13.0",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "express-enforces-ssl": "^1.1.0",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "morgan-json": "^1.1.0",
    "pnpm": "^7.1.7",
    "re2": "^1.17.4",
    "serve-favicon": "^2.5.0",
    "ts-jest": "^28.0.3",
    "typescript": "^4.7.2",
    "winston": "^3.7.2"
  }
}